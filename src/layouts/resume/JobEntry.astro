---
import type { MarkdownLayoutProps } from 'astro';

type Props = MarkdownLayoutProps<{
  companyName: string;
  title: string;
  startDate: number;
  endDate?: number | 'present';
}>;

const { companyName, title, startDate, endDate = 'present' } = Astro.props;
---

<section>
  <h4><strong>{companyName}</strong> | {title} | {startDate} - {endDate}</h4>
  <slot />
</section>
